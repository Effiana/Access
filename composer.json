{
  "name": "effiana/access",
  "license": "proprietary",
  "authors": [
    {
      "name": "Dominik Labudzinski",
      "email": "dominik@labudzinski.com",
      "homepage": "http://labudzinski.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Effiana\\Access\\": "src/"
    }
  },
  "type": "symfony-bundle",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "7.2"
    },
    "preferred-install": {
      "*": "dist"
    },
    "optimize-autoloader": true,
    "sort-packages": true,
    "apcu-autoloader": true
  },
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "ext-pdo": "*",
    "doctrine/data-fixtures": "^1.3",
    "doctrine/orm": "2.*",
    "effiana/migration-bundle": "^0.10.0",
    "knplabs/doctrine-behaviors": "^1.6",
    "symfony/symfony": "4.*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  }
}
